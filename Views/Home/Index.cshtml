@model List<QandA_lesson1.DataStore.Question>
@{
    ViewData["Title"] = "Home Page";
}

<div class="header">
    <h5>UNANSWERED QUESTIONS</h5>
    <button class="primary-button">Ask a question</button>
</div>


@{ foreach (var question in Model)
    {
        <div class="box">
            <h3> @question.Text</h3>
            <p>There seem to be a fair few state management tools...</p>
            <p><i>Asked by @question.User.Username (opens in new tab) on <time> 05/18/2019 2:48:00 PM</time></i></p>
        </div>
    }
}


