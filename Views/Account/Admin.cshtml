@model List<User>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Admin</title>
</head>
<body>
    <ul>
        @{
            foreach (var item in Model)
            {
                  <li>@item.Username : @item.Password</li>
            }
        }
    </ul>
</body>
</html>
